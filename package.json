{
  "name": "skx",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache .vercel/cache",
    "test": "echo \"No tests specified\" && exit 0",
    "test:mcp": "node --test --loader ts-node/esm mcp-server/tests/**/*.test.ts",
    "start:mcp": "tsx mcp-server/index.ts",
    "mcp:list": "node -e \"const cp=require('child_process');const p=cp.spawn('npm',['run','start:mcp'],{stdio:['pipe','pipe','inherit']});p.stdout.on('data',d=>{const s=d.toString().trim();if(!s)return;});setTimeout(()=>{p.stdin.write(JSON.stringify({jsonrpc:'2.0',id:1,method:'initialize'})+'\\n');p.stdin.write(JSON.stringify({jsonrpc:'2.0',id:2,method:'list_tools'})+'\\n');},600);setTimeout(()=>p.kill(),1800);\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "type-check": "tsc --noEmit",
    "db:seed": "psql \"$DATABASE_URL_UNPOOLED\" -v ON_ERROR_STOP=1 -f db/tests/local_seed_min.sql"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.32.0",
    "@types/classnames": "^2.3.0",
    "@types/pg": "^8.15.5",
    "classnames": "^2.5.1",
    "clsx": "2.1.0",
    "framer-motion": "^12.23.12",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.543.0",
    "marked": "^16.2.1",
    "next": "^15.5.0",
    "openai": "^5.19.1",
    "pg": "^8.16.3",
    "puppeteer": "^24.17.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "swr": "^2.3.6",
    "tailwind-merge": "2.2.2",
    "wappalyzer": "^6.10.48",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/express": "^5.0.3",
    "@types/node": "^22.18.6",
    "@types/react": "^19.0.7",
    "autoprefixer": "^10.4.21",
    "express": "^5.1.0",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "tsx": "^4.19.2"
  }
}
